{
  "name": "@owlprotocol/envvars",
  "version": "2.0.2",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/owlprotocol.git"
  },
  "homepage": "https://github.com/owlprotocol/owlprotocol/tree/main/configs/envvars",
  "copyright": "Copyright 2022 Owl Protocol",
  "author": {
    "name": "Leo Vigna",
    "email": "leo.vigna@gmail.com",
    "url": "https://github.com/leovigna"
  },
  "license": "MIT",
  "type": "module",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types/index.d.mts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "browser": "./lib/esm/index.browser.js",
      "import": "./lib/esm/index.js",
      "require": "./lib/cjs/index.js"
    },
    "./browser": {
      "types": "./lib/types/index.browser.d.ts",
      "browser": "./lib/esm/index.browser.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "build": "pnpm run codegen && npm-run-all -p tsc esbuild",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "clean": "rimraf lib dist",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "codegen": "tsx src/genEnvVars.ts"
  },
  "devDependencies": {
    "@owlprotocol/chains": "workspace:*",
    "@owlprotocol/esbuild-config": "workspace:*",
    "@owlprotocol/eslint-config": "workspace:*",
    "@owlprotocol/prettier-config": "workspace:*",
    "@owlprotocol/tsconfig": "workspace:*",
    "@types/node": "^20.5.6",
    "@types/dotenv": "^8.2.0",
    "dotenv": "^16.3.1",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tsx": "^3.12.7",
    "typescript": "4.9.5",
    "vite": "3.0.2"
  }
}
