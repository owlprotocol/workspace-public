{
  "name": "@owlprotocol/ui-components",
  "version": "3.0.0",
  "keywords": [
    "web3",
    "solidity",
    "trpc",
    "react-query"
  ],
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/owlprotocol.git"
  },
  "license": "MIT",
  "author": {
    "name": "Leo Vigna",
    "email": "leo@owlprotocol.xyz",
    "url": "https://github.com/leovigna"
  },
  "files": [
    "dist/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vite build",
    "build:app": "vite build --config vite-app.config.mts",
    "build:watch": "cross-env NODE_OPTIONS='--max-old-space-size=4096' vite build --watch",
    "clean": "rimraf lib dist",
    "dev": "vite --config vite-app.config.mts",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "lint:fix": "eslint 'src/**/*.ts' 'src/**/*.tsx' --fix",
    "madge": "madge src/index.ts -c",
    "preview": "vite preview",
    "start": "vite --config vite-app.config.mts",
    "tsc": "tsc",
    "tsc:trace": "tsc --generateTrace lib/trace && analyze-trace lib/trace",
    "tsc:watch": "tsc -w",
    "tsr:generate": "tsr generate"
  },
  "dependencies": {
    "@owlprotocol/core-firebase": "workspace:*",
    "@owlprotocol/core-trpc": "workspace:*",
    "@owlprotocol/envvars": "workspace:*",
    "@clerk/clerk-react": "^4.24.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "1.0.2",
    "@trpc/react-query": "11.0.0-next-beta.308",
    "@tanstack/react-router": "^1.15.3",
    "@tanstack/react-query": "5.24.8",
    "@tanstack/router-vite-plugin": "^1.15.22",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.1",
    "firebase": "^10.3.1",
    "lodash-es": "4.17.21",
    "lucide-react": "^0.319.0",
    "next-themes": "^0.2.1",
    "reactfire": "^4.2.3",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.3",
    "react-syntax-highlighter": "^15.5.0",
    "sonner": "^1.4.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@owlprotocol/eslint-config": "workspace:*",
    "@owlprotocol/vite-config": "workspace:*",
    "@tanstack/eslint-plugin-query": "^5.20.1",
    "@tanstack/router-devtools": "^1.15.3",
    "@tanstack/router-vite-plugin": "^1.30.0",
    "@types/lodash-es": "4.17.6",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-syntax-highlighter": "^15.5.11",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.17",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "global": "^4.4.0",
    "madge": "^5.0.2",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "trpc-openapi": "^1.2.0",
    "typescript": "5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-externalize-deps": "^0.7.0",
    "vite-plugin-node-polyfills": "^0.14.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^1.4.0",
    "@owlprotocol/esbuild-config": "workspace:*",
    "@owlprotocol/prettier-config": "workspace:*",
    "@owlprotocol/tsconfig": "workspace:*",
    "@typescript/analyze-trace": "^0.10.1",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "tsx": "3.12.7"
  },
  "bugs": {
    "url": "https://github.com/owlprotocol/owlprotocol/issues",
    "email": "leo@owlprotocol.xyz"
  },
  "funding": "https://owlprotocol.xyz",
  "engines": {
    "node": "^18.0.0"
  }
}
