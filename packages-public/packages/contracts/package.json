{
  "name": "@owlprotocol/contracts",
  "version": "2.0.4",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/owlprotocol.git"
  },
  "homepage": "https://github.com/owlprotocol/owlprotocol.git",
  "copyright": "Copyright 2023 Owl Protocol",
  "author": {
    "name": "Leo Vigna",
    "email": "leo.vigna@gmail.com",
    "url": "https://github.com/owlprotocol"
  },
  "license": "MIT",
  "type": "module",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./ethers/factoryClasses": {
      "types": "./lib/types/ethers/factoryClasses.d.ts",
      "require": "./lib/cjs/ethers/factoryClasses.js",
      "import": "./lib/esm/ethers/factoryClasses.js"
    },
    "./ethers/factoryClassesInitializable": {
      "types": "./lib/types/ethers/factoryClassesInitializable.d.ts",
      "require": "./lib/cjs/ethers/factoryClassesInitializable.js",
      "import": "./lib/esm/ethers/factoryClassesInitializable.js"
    },
    "./ethers/factoryInterfaceClasses": {
      "types": "./lib/types/ethers/factoryInterfaceClasses.d.ts",
      "require": "./lib/cjs/ethers/factoryInterfaceClasses.js",
      "import": "./lib/esm/ethers/factoryInterfaceClasses.js"
    },
    "./typechain/ethers/*": {
      "types": "./lib/types/typechain/ethers/*.d.ts",
      "require": "./lib/cjs/typechain/ethers/*.js",
      "import": "./lib/esm/typechain/ethers/*.js"
    },
    "./typechain/ethers": {
      "types": "./lib/types/typechain/ethers/index.d.ts",
      "require": "./lib/cjs/typechain/ethers/index.js",
      "import": "./lib/esm/typechain/ethers/index.js"
    },
    "./zsol": {
      "types": "./lib/types/zsol/index.d.ts",
      "require": "./lib/cjs/zsol/index.js",
      "import": "./lib/esm/zsol/index.js"
    },
    "./factories": {
      "types": "./lib/types/factories/index.d.ts",
      "require": "./lib/cjs/factories/index.js",
      "import": "./lib/esm/factories/index.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*",
    "!lib/forge-std",
    "src/*",
    "LICENSE",
    "README.md",
    "contracts/*"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "build": "npm-run-all to:commonjs build:hardhat build:docs build:typechain to:module build:zsol build:factories build:deployments-export build:interfaceIds build:esm",
    "build:esm": "npm-run-all build:esbuild build:tsc ",
    "build:watch": "npm-run-all -p build:tsc:watch build:esbuild:watch",
    "build:interfaceIds": "rm -f interfaceId/* && tsx src/codegen/runGenerateInterfaceIds.ts",
    "build:deployments-export": "tsx src/codegen/runGenerateDeploymentsExports.ts",
    "build:typechain": "npm-run-all build:typechain:ethers build:typechain:ethers:custom",
    "build:typechain:ethers": "typechain --target=ethers-v5 --node16-modules --show-stack-traces --out-dir=src/typechain/ethers 'src/artifacts/+(contracts|@openzeppelin|@chainlink|@opengsn|@safe-global)/**/+([a-zA-Z0-9_]).json'",
    "build:typechain:ethers:custom": "typechain --target=ethers-v5 --node16-modules --show-stack-traces --out-dir=src/typechain/ethers 'src/artifacts-custom/+(custom|@safe-global)/**/+([a-zA-Z0-9_]).json'",
    "build:docs": "hardhat docgen",
    "build:esbuild": "node esbuild.config.mjs",
    "build:esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "build:tsc": "tsc",
    "build:tsc:watch": "tsc",
    "build:hardhat": "hardhat compile",
    "build:zsol": "tsx src/codegen/runGenerateZodValidators.ts",
    "build:factories": "tsx src/codegen/runGenerateCreate2FactoryWrappers.ts",
    "clean": "rimraf lib bin cache docs-contracts-reference src/artifacts src/typechain src/zsol src/factories",
    "coverage": "hardhat coverage --solcoverjs .solcover.js --testfiles 'test/**/*.test.ts'",
    "coverage:gsn": "hardhat --network local coverage --solcoverjs .solcover.js --testfiles 'test/**/*.test.ts'",
    "test-exec": "hardhat test",
    "test:ERC721Mintable": "npm-run-all to:commonjs test-exec:ERC721Mintable to:module",
    "test-exec:ERC721Mintable": "hardhat test lib/cjs/test/assets/ERC721/ERC721Mintable.test.js",
    "test-grep": "npm-run-all to:commonjs test-grep-exec to:module",
    "test-grep-exec": "hardhat test $TEST_FILE",
    "deploy": "npm-run-all to:commonjs deploy:all to:module",
    "deploy:all": "hardhat deploy",
    "verify": "hardhat run src/scripts/verify.ts",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'",
    "madge": "madge src/index.ts -c",
    "to:commonjs": "sed 's/\"type\": \"module\"/\"type\": \"commonjs\"/g' package.json > ./temp.package.json && mv ./temp.package.json package.json",
    "to:module": "sed 's/\"type\": \"commonjs\"/\"type\": \"module\"/g' package.json > ./temp.package.json && mv -f ./temp.package.json package.json"
  },
  "dependencies": {
    "@openzeppelin/contracts-upgradeable": "4.8.3",
    "@owlprotocol/contracts-proxy": "workspace:*",
    "@owlprotocol/zod-sol": "workspace:*",
    "@chainlink/contracts": "^0.4.1",
    "@safe-global/protocol-kit": "^1.2.0",
    "lodash": "4.17.21",
    "loglevel": "^1.8.1",
    "zod": "3.21.4"
  },
  "peerDependencies": {
    "@chainlink/contracts": "^0.4.1",
    "@ethersproject/abi": "^5.4.0",
    "@ethersproject/contracts": "^5.4.1",
    "@ethersproject/providers": "^5.4.3",
    "@opengsn/contracts": "^2.2.6",
    "ethers": "5.7.2",
    "web3": "1.8.0",
    "web3-core": "1.8.0",
    "web3-eth-contract": "1.8.0",
    "web3-utils": "1.8.0"
  },
  "devDependencies": {
    "@ethereumjs/tx": "^3.5.2",
    "@ethersproject/abi": "^5.5.0",
    "@ethersproject/bignumber": "^5.6.2",
    "@ethersproject/contracts": "^5.6.2",
    "@ethersproject/providers": "^5.5.2",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-verify": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "@owlprotocol/esbuild-config": "workspace:*",
    "@owlprotocol/eslint-config": "workspace:*",
    "@owlprotocol/prettier-config": "workspace:*",
    "@owlprotocol/hardhat-config": "workspace:*",
    "@owlprotocol/utils": "workspace:*",
    "@owlprotocol/utils-ethers": "workspace:*",
    "@owlprotocol/envvars": "workspace:*",
    "@owlprotocol/tsconfig": "workspace:*",
    "@safe-global/safe-core-sdk-types": "^2.2.0",
    "@typechain/ethers-v5": "^11.1.1",
    "@typechain/web3-v1": "^6.0.6",
    "@types/bn.js": "^5.1.1",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.4",
    "@types/crypto-js": "^4.1.1",
    "@types/glob": "^7.2.0",
    "@types/lodash": "4.14.188",
    "@types/mocha": "^9.1.1",
    "@types/node": "^20.5.6",
    "add": "^2.0.6",
    "bn.js": "^5.2.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-ethers": "^0.0.1",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.1.1",
    "csvtojson": "^2.0.10",
    "dotenv": "^16.0.1",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-util": "^7.1.5",
    "ethers": "5.7.2",
    "ganache": "^7.9.1",
    "handlebars": "^4.7.7",
    "hardhat": "2.16.1",
    "hardhat-deploy": "^0.11.10",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-preprocessor": "^0.1.5",
    "ipfs-http-client": "56.0.0",
    "lint-staged": "^11.2.6",
    "madge": "^5.0.1",
    "mocha": "^9.1.4",
    "npm-run-all": "^4.1.5",
    "ow": "^1.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "solc-0.8": "npm:solc@^0.8.12",
    "solidity-coverage": "^0.7.21",
    "solidity-docgen": "^0.6.0-beta.29",
    "truffle": "^5.5.21",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typechain": "8.3.1",
    "typedoc": "^0.21.10",
    "typedoc-neo-theme": "^1.1.1",
    "typescript": "4.9.5",
    "uint8arrays": "^3.0.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.2",
    "web3": "1.8.0",
    "web3-core": "1.8.0",
    "web3-eth-contract": "1.8.0",
    "web3-providers-http": "1.8.0",
    "web3-utils": "1.8.0"
  }
}
