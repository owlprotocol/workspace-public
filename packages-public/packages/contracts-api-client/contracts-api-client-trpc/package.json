{
  "name": "@owlprotocol/contracts-api-client-trpc",
  "version": "2.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/owlprotocol/contracts-api-client-trpc.git"
  },
  "homepage": "https://owlprotocol.github.io/contracts-api-client-trpc",
  "copyright": "Copyright 2023 Owl Protocol",
  "author": {
    "name": "Leo Vigna",
    "email": "leo.vigna@gmail.com",
    "url": "https://github.com/owlprotocol"
  },
  "license": "MIT",
  "type": "module",
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "require": "./lib/cjs/index.js",
      "import": "./lib/esm/index.js"
    },
    "./client": {
      "types": "./lib/types/client.d.ts",
      "require": "./lib/cjs/client.js",
      "import": "./lib/esm/client.js"
    },
    "./react": {
      "types": "./lib/types/react.d.ts",
      "require": "./lib/cjs/react.js",
      "import": "./lib/esm/react.js"
    }
  },
  "files": [
    "dist/*",
    "lib/*",
    "src/*",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "test:watch": "vitest",
    "test:ci": "vitest --run",
    "build": "npm-run-all -p tsc esbuild",
    "build:watch": "npm-run-all -p tsc:watch esbuild:watch",
    "esbuild": "node esbuild.config.mjs",
    "esbuild:watch": "ESBUILD_WATCH=true node esbuild.config.mjs",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "clean": "rimraf lib dist",
    "madge": "madge src/index.ts -c",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint --fix 'src/**/*.ts'"
  },
  "dependencies": {
    "@trpc/client": "10.44.1",
    "@trpc/server": "10.44.1",
    "cross-fetch": "^4.0.0"
  },
  "devDependencies": {
    "@dfns/sdk": "0.1.0-beta.1",
    "@ethersproject/providers": "5.7.2",
    "@owlprotocol/contracts": "workspace:*",
    "@owlprotocol/contracts-api": "workspace:*",
    "@owlprotocol/contracts-api-firebase": "workspace:*",
    "@owlprotocol/crud-firebase": "workspace:*",
    "@owlprotocol/contracts-proxy": "workspace:*",
    "@owlprotocol/dfns-sdk-mock": "workspace:*",
    "@owlprotocol/esbuild-config": "workspace:*",
    "@owlprotocol/eslint-config": "workspace:*",
    "@owlprotocol/prettier-config": "workspace:*",
    "@owlprotocol/safe-kit": "workspace:*",
    "@owlprotocol/shopify-api-mock": "workspace:*",
    "@owlprotocol/tsconfig": "workspace:*",
    "@owlprotocol/envvars": "workspace:*",
    "@shopify/shopify-api": "^7.7.0",
    "@tanstack/react-query": "^4.29.7",
    "@trpc/react-query": "^10.38.4",
    "@types/dotenv": "^8.2.0",
    "@types/node": "^20.5.6",
    "@types/react": "^17.0.40",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "madge": "^5.0.2",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "rimraf": "^3.0.2",
    "trpc-openapi": "^1.2.0",
    "typescript": "4.9.5",
    "vite": "^4.4.9",
    "vitest": "^0.34.2"
  }
}
